      "import": "./dist/vue-sticky-to-parent.js",
      "require": "./dist/vue-sticky-to-parent.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
