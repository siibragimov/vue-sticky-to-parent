  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.24"
