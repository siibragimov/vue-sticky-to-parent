{
  "name": "vue-sticky-to-parent",
  "author": {
    "name": "sayid.ibragimov"
  },
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-sticky-to-parent.umd.cjs",
  "module": "./dist/vue-sticky-to-parent.js",
  "types": "./dist/vue-sticky-to-parent.d.ts",
  "exports": {
